{
    "name": "jukebox",
    "version": "0.1.0",
    "description": "JukeBox is music bot for Discord",
    "repository": {
        "url": "https://github.com/atahanyorganci/jukebox.git",
        "type": "git"
    },
    "main": "dist",
    "type": "module",
    "scripts": {
        "lint": "eslint --fix src/**/*.ts src/*.ts",
        "format": "prettier --write 'src/**/*.ts' 'src/*.ts'",
        "build": "node build.mjs",
        "build:watch": "nodemon -w src -e ts --exec 'node build.mjs'",
        "check": "tsc",
        "check:watch": "tsc --watch --preserveWatchOutput",
        "start": "node dist/bundle.cjs",
        "start:watch": "nodemon -w dist -e cjs,cjs.map --exec 'node dist/bundle.cjs'",
        "dev": "concurrently -k -n BUILD,CHECK,BOT \"pnpm run build:watch\" \"pnpm run check:watch\" \"pnpm run start:watch\"",
        "test": "vitest"
    },
    "keywords": [
        "bot",
        "discord",
        "music"
    ],
    "author": "Atahan YorgancÄ±",
    "license": "MIT",
    "dependencies": {
        "@discordjs/builders": "^1.2.0",
        "@discordjs/voice": "^0.11.0",
        "discord.js": "^14.3.0",
        "dotenv": "^16.0.2",
        "ffmpeg-static": "^5.1.0",
        "googleapis": "^107.0.0",
        "libsodium-wrappers": "^0.7.10",
        "opusscript": "^0.0.8",
        "winston": "^3.8.2",
        "ytdl-core": "^4.11.2",
        "zod": "^3.19.0"
    },
    "devDependencies": {
        "@types/node": "^18.7.16",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "concurrently": "^7.4.0",
        "esbuild": "^0.17.19",
        "eslint": "^8.23.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "prettier-plugin-organize-imports": "^3.2.2",
        "typescript": "^4.8.3",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.31.1"
    },
    "volta": {
        "node": "18.16.0"
    }
}
