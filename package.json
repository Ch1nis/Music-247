{
    "name": "jukebox",
    "version": "0.1.0",
    "description": "JukeBox is music bot for Discord",
    "repository": {
        "url": "https://github.com/atahanyorganci/jukebox.git",
        "type": "git"
    },
    "main": "dist",
    "type": "module",
    "scripts": {
        "lint": "eslint --fix src/**/*.ts src/*.ts",
        "format": "prettier --write 'src/**/*.ts' 'src/*.ts'",
        "build": "node build.mjs",
        "build:watch": "nodemon -w src -e ts --exec 'node build.mjs'",
        "check": "tsc",
        "check:watch": "tsc --watch --preserveWatchOutput",
        "start": "node dist/bundle.cjs",
        "start:watch": "nodemon -w dist -e cjs,cjs.map --exec 'node dist/bundle.cjs'",
        "dev": "concurrently -k -n BUILD,CHECK,BOT \"pnpm run build:watch\" \"pnpm run check:watch\" \"pnpm run start:watch\"",
        "test": "vitest"
    },
    "keywords": [
        "bot",
        "discord",
        "music"
    ],
    "author": "Atahan YorgancÄ±",
    "license": "MIT",
    "dependencies": {
        "@discordjs/builders": "^1.6.3",
        "@discordjs/voice": "^0.16.0",
        "discord.js": "^14.11.0",
        "dotenv": "^16.3.1",
        "ffmpeg-static": "^5.1.0",
        "libsodium-wrappers": "^0.7.11",
        "opusscript": "^0.0.8",
        "winston": "^3.10.0",
        "ytdl-core": "^4.11.5",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@types/node": "^20.4.2",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "concurrently": "^8.2.0",
        "esbuild": "^0.18.15",
        "eslint": "^8.45.0",
        "eslint-plugin-prettier": "^5.0.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.0",
        "prettier-plugin-organize-imports": "^3.2.3",
        "typescript": "^5.1.6",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.33.0"
    },
    "volta": {
        "node": "18.16.0"
    }
}
