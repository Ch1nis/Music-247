{
    "name": "jukebox",
    "version": "0.1.0",
    "description": "JukeBox is music bot for Discord",
    "repository": {
        "url": "https://github.com/atahanyorganci/jukebox.git",
        "type": "git"
    },
    "main": "dist",
    "scripts": {
        "lint": "eslint --fix src/**/*.ts src/*.ts",
        "format": "prettier --write 'src/**/*.ts' 'src/*.ts'",
        "build": "tsc",
        "build:watch": "tsc --watch --preserveWatchOutput",
        "start": "node dist",
        "start:watch": "nodemon -w dist -e js,js.map --exec 'node dist'",
        "dev": "concurrently -k -n BUILD,BOT \"yarn run build:watch\" \"yarn run start:watch\"",
        "prepare": "ts-patch install -s",
        "test": "jest"
    },
    "keywords": [
        "bot",
        "discord",
        "music"
    ],
    "author": "Atahan YorgancÄ±",
    "license": "MIT",
    "dependencies": {
        "@discordjs/builders": "^1.2.0",
        "@discordjs/voice": "^0.11.0",
        "discord.js": "^14.3.0",
        "dotenv": "^16.0.2",
        "googleapis": "^107.0.0",
        "libsodium-wrappers": "^0.7.10",
        "opusscript": "^0.0.8",
        "winston": "^3.8.2",
        "ytdl-core": "^4.11.2",
        "zod": "^3.19.0"
    },
    "devDependencies": {
        "@tsconfig/node16": "^1.0.3",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.16",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "concurrently": "^7.4.0",
        "eslint": "^8.23.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.0.2",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "prettier-plugin-organize-imports": "^3.2.2",
        "ts-jest": "^29.0.0",
        "ts-patch": "^2.0.2",
        "typescript": "^4.8.3",
        "typescript-transform-paths": "^3.3.1"
    },
    "volta": {
        "node": "16.17.0",
        "yarn": "1.22.19"
    }
}
